{
    "name": "informix4gl",
    "displayName": "Informix 4GL",
    "description": "Sintaxis para Informix 4GL en Vs Code",
    "icon": "images/logo.jpg",
    "version": "0.0.3",
    "publisher": "CristianRengifo",
    "repository": {
        "type": "git",
        "url": "https://github.com/INGCRENGIFO/VsCodeInformix4GL"
    },
    "keywords": [
        "Informix",
        "Informix 4GL",
        "4GL"
    ],
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "4GL",
            "aliases": ["Informix 4GL", "4GL"],
            "extensions": [".4gl",".per"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "4GL",
            "scopeName": "source.4GL",
            "path": "./syntaxes/4GL.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "4GL",
                "path": "./snippets/snippets.json"
            }
        ],
        "configurationDefaults": {
            "[4GL]": {
                "files.encoding": "cp437"
            }
        }
    }
}